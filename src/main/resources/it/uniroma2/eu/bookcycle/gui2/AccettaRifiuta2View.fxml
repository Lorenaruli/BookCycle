<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.uniroma2.eu.bookcycle.controller.gui2.AccettaRifiuta2ViewController">

    <children>
        <Label layoutX="20" layoutY="20" text="Vuoi accettare o rifiutare questa proposta?" />

        <Label fx:id="dettagliProposta" layoutX="20" layoutY="60" text="LibroOfferto per  LibroRichiesto" />

        <Button fx:id="accettaButton" layoutX="80" layoutY="120" mnemonicParsing="false" onAction="#accettaProposta" text="Accetta" />

        <Button fx:id="rifiutaButton" layoutX="220" layoutY="120" mnemonicParsing="false" onAction="#rifiutaProposta" text="Rifiuta" />

        <Button fx:id="annullaButton" layoutX="320" layoutY="160" mnemonicParsing="false" onAction="#annulla" text="Annulla" />
    </children>
</AnchorPane>
